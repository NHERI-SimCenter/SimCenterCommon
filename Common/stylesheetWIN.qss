/*----------------------------------------------------------------------------*/
/* QPushButton                                                                */
/*----------------------------------------------------------------------------*/

QPushButton {
    font-size: 10pt;
    color: white;
    border-style: outset;
    background-color: #64B5F6;
    border-style: outset;
    border-width: 1px;
    border-radius: 2px;
    border-color: #607D8B;
    font-weight: bold;
    padding: 3px;
}

QPushButton:pressed {
    background-color: #B0BEC5;
    border-style: inset;
    border-color: black;
}


/*----------------------------------------------------------------------------*/
/* QGroupBox                                                                  */
/*----------------------------------------------------------------------------*/

QGroupBox {
    font-size: 10pt;
    font-weight: bold;
}


/*----------------------------------------------------------------------------*/
/* QTabWidget                                                                 */
/*----------------------------------------------------------------------------*/

QTabWidget::pane {
    background-color: #ECECEC;
    border: 1px solid rgb(244, 244, 244);
}

QTreeView#treeViewOnTheLeft{
    background: rgb(79, 83, 89);

    /* set the font properties*/
    font-size:12pt;
    color: white;
}

QTreeView#treeViewOnTheLeft::item:hover {
    background: rgb(69, 187, 217);
}

QTreeView#treeViewOnTheLeft::item:selected:active{
    background: rgb(63, 147, 168);
}

QTreeView#treeViewOnTheLeft::item:selected:!active {
    background: rgb(63, 147, 168);
}

QComboBox {
    font-size: 10pt;
    border:  solid gray;
    /*min-width: 6em;*/
}

QComboBox:editable {
    background: white;
}

QComboBox:!editable, QComboBox::drop-down:editable {
     background: white;
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    background: rgb(240, 240, 240);
}


